<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ADDSMock.Applications.Interactive.Views.Explorer.ExplorerPaneView"
             x:ClassModifier="internal">

    <Border BorderBrush="#424242" BorderThickness="1,0,0,0">
        <Grid RowDefinitions="Auto, *">
            <TextBlock Grid.Row="0" Text="EXPLORER" Margin="8,4,0,4" VerticalAlignment="Center" FontWeight="Light"
                       Foreground="#c0c0c0" />

            <TextBlock Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center"
                       Text="Not available" />
        </Grid>

    </Border>


</UserControl>
